    <!-- Page info -->
    <div class="page-info-section set-bg" data-setbg="images/IICT4.png"></div>
    <!-- Page info end -->

    
    <!----Profile Section-->
    <% if (user) { %>
    <section class="single-course spad pb-0">
        <div class="container">
          <div class="course-meta-area">
            <div class="row">
              <div class="col-lg-10 offset-lg-1">
                <h3>User Information</h3>
                <div class="course-metas">
                  <div class="course-meta">
                    <div class="course-author">
                      <div
                        class="ca-pic set-bg"
                        data-setbg="img/authors/2.jpg"
                      ></div>
                      <h6>Name</h6>
                      <p><%= user.name %></p>
                    </div>
                  </div>
                  <div class="course-meta">
                    <div class="cm-info">
                      <h6>Registration No.</h6>
                      <p><%= user.reg_no %></p>
                    </div>
                  </div>
                  <div class="course-meta">
                    <div class="cm-info">
                      <h6>Email</h6>
                      <p><%= user.email %></p>
                    </div>
                  </div>
                  <div class="course-meta">
                    <div class="cm-info">
                      <h6>Institution</h6>
                      <p>SUST</p>
                    </div>
                  </div>
                </div>
                <a href="/profileUpdate" class="site-btn buy-btn">Update Profile</a>
              </div>
            </div>
          </div>
        </div>
    </section>
    <% } %>
